; AGS2 run script
; ---------------
;RunFile=A-Games:E/EyeOfTheBeholderDe/EyeOfTheBeholderDe.Slave

; Favourites Mode
IF EXISTS ENV:ags2favouritesmode
  execute S:AGS2Favourites "E" "Eye of the Beholder DE"
  IF $ags2favouritesmode EQ "add"
    SKIP end
  ENDIF
  IF $ags2favouritesmode EQ "remove"
    SKIP end
  ENDIF
ENDIF

; Start AGS2 Run Pre Script, if it exists
IF EXISTS S:AGS2RunPre
  execute S:AGS2RunPre
ENDIF

; Run WHDLoad
cd "A-Games:E/EyeOfTheBeholderDe/"
IF EXISTS ENV:whdloadargs
  whdload "EyeOfTheBeholderDe.Slave" $whdloadargs
ELSE
  whdload "EyeOfTheBeholderDe.Slave"
ENDIF

; Start AGS2 Run Post Script, if it exists
IF EXISTS S:AGS2RunPost
  execute S:AGS2RunPost
ENDIF

; End
LAB end
