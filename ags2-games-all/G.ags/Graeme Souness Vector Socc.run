cd "A-Games:G/GraemeSounessVectorSoccer/"
IF EXISTS ENV:whdloadargs
  whdload "GraemeSounessVectorSoccer.Slave" $whdloadargs
ELSE
  whdload "GraemeSounessVectorSoccer.Slave"
ENDIF

; Start AGS2 Run Post Script, if it exists
